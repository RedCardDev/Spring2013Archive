<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kickshot"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ballchiphome"
        android:label="@string/kickshot"
        android:theme="@style/AppTheme" android:allowTaskReparenting="false">
        <activity
            android:name="KickShot"
            android:label="@string/kickshot" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="GameBoardLayout"></activity>
    </application>

</manifest>